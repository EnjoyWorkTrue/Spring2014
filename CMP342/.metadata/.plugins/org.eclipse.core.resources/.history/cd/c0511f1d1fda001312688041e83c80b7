package practiceSingleTone;

import java.util.ArrayList;
import java.util.Set;

public class ARRRRY {

	public static void main(String[] args) {
		ArrayList<String> s = new ArrayList<String>();
		s.add("hi");
		s.add("joe");
		s.add("aaa");
		s.add("bbb");
		for(String a : s){
			System.out.println(a);
			if(a.equals("hi")){
				remove(a);
			}
		}
		for(String a:s){
			System.out.println(a);
		}
		
	
	}
}
